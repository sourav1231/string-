#include<bits/stdc++.h>
using namespace std;
string reverse(string s,int start,int end)
{
while(start<=end)
{
    int temp;
    temp=s[start];
    s[start]=s[end];
 s[end]=temp;
    start++;
    end--;
    
}
return s;
  
}
int main()
{
 string str;
getline(cin,str);
int i=0;
  int n=str.length();
  str = reverse(str,0,n-1);
 cout<<str;
 
}
